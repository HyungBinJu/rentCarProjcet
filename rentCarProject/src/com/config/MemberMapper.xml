<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.member">
	
	<select id="idcheck" parameterType="hashmap" resultType="int">
     select count(*)
     from memberinfo
     where userid = #{userid} 
     and passwd = #{passwd}
	</select>
</mapper>